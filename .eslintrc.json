{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ]
  },
  "globals": {
    "expect": true,
    "mockReq": true,
    "mockRes": true,
    "Model": true
  }
}